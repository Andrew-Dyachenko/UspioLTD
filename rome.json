{
	"$schema": "https://docs.rome.tools/schemas/12.1.3/schema.json",
	"files": {
		"ignore": [
			"dist/",
			".parcel-cache/",
			"node_modules/"
		]
	},
	"formatter": {
		"indentSize": 4
	},
	"organizeImports": {
		"enabled": true
	},
	"linter": {
		"enabled": true,
		"rules": {
			"recommended": true
		}
	},
	"javascript": {
		"formatter": {
			"quoteStyle": "single",
			"trailingComma": "none"
		}
	}
}